    <!-- PLAN DE TRATAMIENTO -->
    <h5>PLAN DE TRATAMIENTO</h5><hr>
    <div class="flex-grid">
        <div class="row">
            {{#if plan_tratamiento.en_observacion}}
            <div class="cell auto-size">
                <span>1.- En observación:</span> <span style="border-bottom: 1px solid; color: red;">X</span>
            </div>
            {{/if}}
            {{#if plan_tratamiento.ortodoncia}}
            <div class="cell auto-size">
                <span>Ortodoncia:</span> <span style="border-bottom: 1px solid; color: red;">X</span>
            </div>
            {{/if}}
            {{#if plan_tratamiento.ortognatica}}
            <div class="cell auto-size">
                <span>Ortognatica:</span> <span style="border-bottom: 1px solid; color: red;">X</span>
            </div>
            {{/if}}
            {{#if plan_tratamiento.compromiso}}
            <div class="cell auto-size">
                <span>Compromiso:</span> <span style="border-bottom: 1px solid; color: red;">X</span>
            </div>
            {{/if}}
            {{#if plan_tratamiento.pronostico}}
            <div class="cell auto-size">
                <span>2.- Pronóstico:</span> <span style="border-bottom: 1px solid;">{{plan_tratamiento.pronostico}}</span>
            </div>
            {{/if}}
        </div>
        <div class="row">
            {{#if plan_tratamiento.labio_ph}}
            <div class="cell auto-size">
                <span>3.- Labio PH:</span> <span style="border-bottom: 1px solid;">{{plan_tratamiento.labio_ph}}</span>
            </div>
            {{/if}}
            {{#if plan_tratamiento.sindrome}}
            <div class="cell auto-size">
                <span>4.- Sindrome:</span> <span style="border-bottom: 1px solid;">{{plan_tratamiento.sindrome}}</span>
            </div>
            {{/if}}
            {{#if plan_tratamiento.observaciones}}
            <div class="cell auto-size">
                <span>5.- Observaciones:</span> <span style="border-bottom: 1px solid;">{{plan_tratamiento.observaciones}}</span>
            </div>
            {{/if}}
        </div>
        <div class="row">
