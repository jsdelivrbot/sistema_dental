                <span>Brackets:</span> <span style="border-bottom: 1px solid;">{{plan_tratamiento.brakets}}</span>
            </div>
            {{/if}}
            {{#if plan_tratamiento.slot}}
            <div class="cell auto-size">
                <span>Slot:</span> <span style="border-bottom: 1px solid;">{{plan_tratamiento.slot}}</span>
            </div>
            {{/if}}
        </div>
        <div class="row">
            {{#if_eq plan_tratamiento.extra "con_extra" }}
